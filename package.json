{
  "name": "ode-ngjs-front",
  "version": "0.1.0-explorer.6",
  "description": "Open Digital Education Frontend Framework",
  "main": "dist/ts/index.js",
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "copy-libs": "cpx \"node_modules/{angular,angular-*,html5-boilerplate/dist}/**/*\" lib -C",
    "compile": "npx tsc --build ./tsconfig.json",
    "test": "npm run copy-libs && npm run compile",
    "jasmine": "npx jasmine --config=src/test/config/jasmine.json",
    "karma": "npx karma start src/test/config/karma.js",
    "build": "cpx \"src/ts/**/*.html\" dist/ts/ -C && npx webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/ode-ngjs-front.git"
  },
  "author": "Open Digital Education",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/ode-ngjs-front/issues"
  },
  "homepage": "https://github.com/opendigitaleducation/ode-ngjs-front#readme",
  "files": [
    "dist/ts/**/*",
    "dist/examples/**/*"
  ],
  "dependencies": {
    "angular": "1.7.9",
    "angular-route": "1.7.9",
    "angular-sanitize": "1.7.9",
    "core-js": "^3.9.1"
  },
  "devDependencies": {
    "@types/angular": "^1.7.4",
    "@types/angular-mocks": "^1.7.0",
    "@types/jasmine": "^3.6.4",
    "angular-mocks": "^1.7.9",
    "cpx": "^1.5.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "http-server": "^0.12.3",
    "jasmine-core": "^3.6.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "ode-ts-client": "^0.1.0-explorer.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0"
  }
}
